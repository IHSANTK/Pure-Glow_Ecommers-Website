                                <form id="productForm" action="/product-edit/<%= data._id %>" method="post" enctype="multipart/form-data">
                                    <div class="row g-3 align-items-center">
                                        <div class="col-md-6">
                                            <label class="form-label">Name</label>
                                            <input type="text" class="form-control" name="ProductName" value="<%= data.productName %>">
                                            <div id="nameError" class="text-danger"></div>
                                        </div>
                                        <div class="col-md-6">
                                            <label class="form-label">Price</label>
                                            <input type="text" class="form-control" name="Price" value="<%= data.productPrice %>">
                                            <div id="priceError" class="text-danger"></div>
                                        </div>
                                        <div class="col-md-6">
                                            <label class="form-label">Description</label>
                                            <input type="text" class="form-control" name="description" value="<%= data.description %>">
                                            <div id="descriptionError" class="text-danger"></div>
                                        </div>
                                        <div class="col-md-6">
                                            <label class="form-label">StockCount</label>
                                            <input type="text" class="form-control" name="stockcount" value="<%= data.stockcount %>">
                                            <div id="descriptionError" class="text-danger"></div>
                                        </div>
                                        <div class="col-md-12">
                                            <label class="form-label">Category</label>
                                            <input type="text" class="form-control" name="category" value="<%= data.category %>">
                                            <div id="categoryError" class="text-danger"></div>
                                        </div>
                                    </div>
                                    <div class="card-header py-3 d-flex justify-content-between bg-transparent border-bottom-0">
                                        <h6 class="mb-0 fw-bold">Images</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="row g-3 align-items-center">
                                            <div class="col-md-12">
                                                <label class="form-label">Update Product Images</label><br>
                                                <div class="col-md-12">
                                                    
                                                    <!-- <small class="d-block text-muted mb-2">Only portrait or square images, 2M max and 2000px max-height.</small> -->
                                                    
                                                    <!-- Display existing images -->
                                                  
                                                    <% data.image.forEach(image => { %>
                                                        <img src="<%= image %>" alt="Existing Image" style="max-width: 100px; max-height: 100px;"><br>
                                                        <!-- Hidden input fields to pass existing image URLs to server-side -->
                                                        <input type="hidden" name="image" value="<%= image %>">
                                                    <% }); %>
                                                    
                                
                                                    <!-- Allow users to upload new images -->
                                                    <input type="file" name="image" id="input-file-to-destroy" class="dropify" data-allowed-formats="portrait square" data-max-file-size="2M" data-max-height="2000">
                                                    <!-- <small class="d-block text-muted mb-2">Only portrait or square images, 2M max and 2000px max-height.</small> -->
                                                    <input type="file" name="image" id="input-file-to-destroy" class="dropify" data-allowed-formats="portrait square" data-max-file-size="2M" data-max-height="2000">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <button type="submit" class="btn btn-primary btn-set-task w-sm-100 py-2 px-5 text-uppercase mt-3">Save</button>
                                </form>
                                
                                <script>
                                    document.getElementById('productForm').addEventListener('submit', function(event) {
                                        let isValid = true;
                                
                                        // Check if Name field is empty
                                        const name = document.querySelector('input[name="ProductName"]').value.trim();
                                        const nameError = document.getElementById('nameError');
                                        nameError.textContent = '';
                                        if (!name) {
                                            nameError.textContent = 'Name is required';
                                            isValid = false;
                                        }
                                
                                        // Check if Price field is empty
                                        const price = document.querySelector('input[name="Price"]').value.trim();
                                        const priceError = document.getElementById('priceError');
                                        priceError.textContent = '';
                                        if (!price) {
                                            priceError.textContent = 'Price is required';
                                            isValid = false;
                                        }
                                
                                        // Check if Description field is empty
                                        const description = document.querySelector('input[name="description"]').value.trim();
                                        const descriptionError = document.getElementById('descriptionError');
                                        descriptionError.textContent = '';
                                        if (!description) {
                                            descriptionError.textContent = 'Description is required';
                                            isValid = false;
                                        }
                                
                                        // Check if Category field is empty
                                        const category = document.querySelector('input[name="category"]').value.trim();
                                        const categoryError = document.getElementById('categoryError');
                                        categoryError.textContent = '';
                                        if (!category) {
                                            categoryError.textContent = 'Category is required';
                                            isValid = false;
                                        }
                                
                                        if (!isValid) {
                                            event.preventDefault(); // Prevent form submission if any field is empty
                                        }
                                    });
                                </script>


                                         
                                    
